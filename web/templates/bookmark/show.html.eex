<h2>Show bookmark</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @bookmark.title %>
  </li>

  <li>
    <strong>Url:</strong>
    <%= @bookmark.url %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @bookmark.description %>
  </li>

</ul>

<%= link "Edit", to: bookmark_path(@conn, :edit, @bookmark) %>
<%= link "Back", to: bookmark_path(@conn, :index) %>
